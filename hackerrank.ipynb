    "import numpy\n",
    "def arrays(arr):\n",
    "    li=[]\n",
    "    for i in range(0,len(arr),3):\n",
    "        li.append([int(a) for a in arr[i:i+3]])\n",
    "    ac=numpy.array(li)\n",
    "    return ac     \n",
