    "import numpy\n",
    "def arrays(arr):\n",
    "    b = numpy.array(arr,float)\n",
    "    li=[]\n",
    "    for i in range(len(b),0,-1):\n",
    "        li.append(b[i-1])\n",
    "    bc=numpy.array(li,float)\n",
    "    return bc\n",
    "\n",
    "arr = input().strip().split(' ')\n",
    "result = arrays(arr)\n",
    "print(result)"
